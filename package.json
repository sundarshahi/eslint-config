{
  "name": "@sundarshahi/eslint-config",
  "version": "3.0.0",
  "description": "Sundar's ESLint config (forked from antfu/eslint-config)",
  "license": "MIT",
  "author": {
    "name": "Sundar Shahi Thakuri",
    "url": "https://github.com/sundarshahi",
    "email": "shahithakurisundar@gmail.com"
  },
  "contributors": [
    "Anthony Fu <anthonyfu117@hotmail.com> (https://github.com/antfu/)"
  ],
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/sundarshahi/eslint-config",
  "packageManager": "pnpm@8.12.0",
  "type": "module",
  "bin": "./bin/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "watch": "tsup --format esm,cjs --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=8.40.0",
    "eslint-plugin-cypress": ">=2.0.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-svelte": "^2.34.1",
    "eslint-plugin-toml": "^0.8.0",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "eslint-processor-vue-blocks": "^0.1.1",
    "svelte-eslint-parser": "^0.33.1",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.3.2"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-cypress": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-toml": {
      "optional": true
    },
    "eslint-plugin-vitest": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "toml-eslint-parser": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@antfu/install-pkg": "^0.3.1",
    "@eslint-types/jsdoc": "46.8.2-1",
    "@eslint-types/typescript-eslint": "^7.0.2",
    "@eslint-types/unicorn": "^51.0.1",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-yml": "^1.12.2",
    "globals": "^14.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "yaml-eslint-parser": "^1.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-plugin-prettier": "^5.0.1-1",
    "@antfu/ni": "^0.21.12",
    "@stylistic/eslint-plugin-migrate": "^1.6.3",
    "@sundarshahi/eslint-config": "workspace:*",
    "@types/eslint": "^8.56.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.24",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.4.0",
    "eslint": "npm:eslint-ts-patch@^8.57.0-0",
    "eslint-flat-config-viewer": "^0.1.11",
    "eslint-plugin-format": "^0.1.0",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "react": "^18.2.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.10.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "vue": "^3.4.21"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "pre-push": "pnpm lint && pnpm vitest run"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
